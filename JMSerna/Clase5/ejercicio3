var num1 = parseInt(prompt("Inserta valor 1"));
var num2 = parseInt(prompt("Inserta valor 2"));
var num3 = parseInt(prompt("Inserta valor 3"));


if(num1 < 0){
	console.log("El resultado es la suma de los dos primeros: "+(num2+num3));
}else{
	console.log("El resultado el la multiplicaciÃ³n de todos: "+(num1*num2*num3));
}
