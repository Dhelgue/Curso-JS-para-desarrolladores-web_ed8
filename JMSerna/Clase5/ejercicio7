var ano = parseInt(prompt("Inserte un año"));

function comparar(ano){
	

	//console.log((ano %4 == 0)&& (ano %100 !== 0));
	//console.log((ano %100 == 0) && (ano %400 == 0));
	
	if((ano %4 == 0)&& (ano %100 !== 0)){
		
		alert("El año es bisiesto")
		
	}else if((ano %100 == 0) && (ano %400 == 0)){
		
		alert("El año es bisiesto");
	}else{
		alert("Es NO es bisiesto");
	}
	
}
comparar(ano);
