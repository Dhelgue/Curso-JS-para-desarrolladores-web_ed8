Algoritmo Ejercicio1
	
	nsuma1<-6
	nsuma2<-4
	resultadosuma<-nsuma1 + nsuma2
	Escribir "Resultado suma : " resultadosuma
	
	nresta1<-5
	nresta2<-2
	resultadoresta<-nresta1-nresta2
	Escribir "Resultado resta : " resultadoresta
	
	ndivision1<-10
	ndivision2<-5
	resultadodivision<-ndivision1/ndivision2
	Escribir "Resultado divisiÃ³n : " resultadodivision
	
	nproducto1<-5
	nproducto2<-5
	resultadoproducto<-nproducto1*nproducto1
	Escribir "Resultado producto : " resultadoproducto
	
FinAlgoritmo


