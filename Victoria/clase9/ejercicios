var arrayIm = []
var arrayPar = []

for (var i=0; i<100; i++){
	var azar=Math.floor(Math.random() * (11 - 0)) + 0;
	if (azar%2 === 0){
	arrayPar.push(azar)
	}
	else {
	arrayIm.push(azar)
	}
}

console.log(arrayIm);
console.log(arrayPar);


